{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": { "runtime": "3.11" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  {
  "build": {
    "env": {
      "PYTHON_VERSION": "3.10"
    }
  }
}
